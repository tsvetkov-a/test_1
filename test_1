create or replace function tabel_dept_id(dept_id number) return number is
  d_id number;
begin
  select id into d_id from (
    select level l, max(level) over () ml, id
    from (select id, case when id = 200335 then null else rd_id end rd_id from tabel_depts)
    start with id = dept_id
    connect by id = prior rd_id
  ) where l = ml-1;
  return d_id;
exception
  when no_data_found then return dept_id;
end tabel_dept_id;
