select  x.*
        from iyastrebov.xml_labels m
        , xmltable(xmlnamespaces('http://asguf.mos.ru/rkis_gu/coordinate/v6_1/' as "ns1", 'http://mos.ru/pgu/ru/application/mggt/086202/' as "ns5")
          , '/ns1:CoordinateMessage[1]/ns1:CoordinateDataMessage[1]/ns1:SignService[1]/ns1:CustomAttributes/ns1:underground_facilitieslist[1]/ns1:underground_facilities/'
            passing m.msg columns
              ServiceProperties xmltype path 'ns5:ServiceProperties'
            , namepoint object varchar2(255) path 'ns5:namepoint object'
            , wayofwork ..... path 'ns5:wayofwork'
            , basementdepth ..... path 'ns5:basementdepth'
            , numberofpoint ..... path 'ns5:numberofpoint'
            , event ...... path 'ns5:event'
            , overground varchar2(10) path 'ns5:overground'
            , underground varchar2(10) path 'ns5:underground'
          ) x;
